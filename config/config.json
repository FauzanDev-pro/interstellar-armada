{
    "version": "0.1.0:136+refactoring-66 (2016.02.20.)",
    "logVerbosity": 1,
    "folders": {
        "config": "config/",
        "css": "css/",
        "data": "data/",
        "screen": "{{data}}/screens/",
        "component": "{{data}}/components/",
        "model": "{{data}}/models/",
        "shader": "{{data}}/shaders/",
        "texture": "{{data}}/textures/",
        "level": "{{data}}/levels/",
        "environment": "{{data}}/levels/environments/"
    },
    "configFiles": {
        "settings": {
            "filename": "settings.json",
            "folder": "config"
        }
    },
    "dataFiles": {
        "graphics": {
            "resources": {
                "filename": "resources.json",
                "folder": "data"
            }
        },
        "logic": {
            "classes": {
                "filename": "classes.json",
                "folder": "data"
            },
            "environments": {
                "filename": "environments.json",
                "folder": "environment"
            },
            "levels": {
                "folder": "level",
                "filenames": [
                    "level.json"
                ]
            }
        }
    }
}
